{
	"luci-app-dnscrypt-proxy2": {
		"description": "Grant UCI access for luci-app-dnscrypt-proxy2",
		"read": {
			"uci": [ "*" ],
			"file": {
				"/etc/config": ["list"],
				"/etc/config/*": ["read"]
				"/var/etc": ["list"],
				"/var/etc/*": ["read"]
			}
		},
		"write": {
			"uci": [ "*" ]
		}
	}
}
